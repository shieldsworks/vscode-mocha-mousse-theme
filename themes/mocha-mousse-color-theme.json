{
  "name": "Mocha Mousse",
  "type": "dark",
  "colors": {
    "editor.background": "#291A15",
    "editor.foreground": "#F7E8E1",
    "activityBar.background": "#1D1411",
    "sideBar.background": "#291A15",
    "sideBar.foreground": "#DCCCBD",
    "titleBar.activeBackground": "#9E7967",
    "statusBar.background": "#9E7967",
    "statusBar.foreground": "#F7E8E1",
    "tab.activeBackground": "#3D2820",
    "tab.inactiveBackground": "#291A15",
    "editorGroupHeader.tabsBackground": "#1D1411",
    "input.background": "#3D2820",
    "dropdown.background": "#3D2820",
    "panel.background": "#291A15",
    "badge.background": "#B14C32",
    "badge.foreground": "#F7E8E1",
    "button.background": "#B14C32",
    "button.foreground": "#F7E8E1"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#A89986"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#CCA98D"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#B14C32"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#E0C7A8"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#DCCCBD"
      }
    },
    {
      "name": "Class names",
      "scope": ["entity.name.type", "entity.name.class"],
      "settings": {
        "foreground": "#CCA98D"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "variable.other.constant"],
      "settings": {
        "foreground": "#B14C32"
      }
    }
  ]
}
